{
  "version": 2,
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/widget.js", "destination": "/public/widget.js" },
    { "source": "/embed.css", "destination": "/public/embed.css" },
    { "source": "/embed.js", "destination": "/public/embed.js" },
    { "source": "/embed(.*)", "destination": "/public/embed.html" },
    { "source": "/demo", "destination": "/public/demo.html" },
    { "source": "/", "destination": "/public/demo.html" }
  ],
  "headers": [
    {
      "source": "/embed(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=0, s-maxage=31536000, immutable" }
      ]
    }
  ]
}


